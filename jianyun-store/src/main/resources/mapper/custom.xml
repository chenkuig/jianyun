<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.demo.mapper.CusterMapper">
  <resultMap id="BaseResultMap" type="com.example.demo.bean.Custom">
      <id column="cust_id" jdbcType="BIGINT" property="id" />
	  <result column="cust_name"	jdbcType="VARCHAR"	        property="custName" />
	  <result column="cust_mobile"	jdbcType="VARCHAR"		    property="custMobile" />
	  <result column="gender" jdbcType="INTEGER"		        property="gender" />
  </resultMap>
  <select id="getList" resultMap="BaseResultMap">
    select * from t_cust
  </select>
</mapper>